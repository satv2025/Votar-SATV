{
  "version": 2,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path*.html",
      "destination": "/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:folder",
      "destination": "/:folder/:folder.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    }
  ]
}